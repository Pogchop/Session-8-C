#include <stdio.h>

int main()
{
    char yeet;
    printf("Enter a character: ");
    scanf("%c",&yeet);
    switch(yeet)
    {
        case 'A':
        case 'a':
        printf("Ada");
        break;
        case 'B':
        case 'b':
        printf("Basic");
        break;
        case 'C':
        case 'c':
        printf("COBOL");
        break;
        case 'D':
        case 'd':
        printf("dBase III");
        break;
        case 'F':
        case 'f':
        printf("Fortran");
        break;
        case 'P':
        case 'p':
        printf("Pascal");
        break;
        case 'V':
        case 'v':
        printf("Visual C++");
        break;
        default:
        printf("Try again");
        break;
        
    }
    return 0;
}
